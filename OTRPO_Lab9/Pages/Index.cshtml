@page
@model IndexModel
@{
    ViewData["Title"] = "Chat Room";
}

<div id="app">
    <h1>Chat Room</h1>
    <div id="online-users">
        <h2>Online Users</h2>
        <ul id="users-list"></ul>
    </div>
    <div id="chat-box">
        <h2>Messages</h2>
        <div id="messages"></div>
        <input type="text" id="message-input" placeholder="Type a message">
        <button id="send-button">Send</button>
    </div>
</div>

